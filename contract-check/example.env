# DeepSeek 提供的 OpenAI 兼容接口
# 如你在企业网关/代理，需要改成对应地址
DEEPSEEK_API_KEY=your_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat

# 使用方式（策略模式）：
#   1. create_chat_model(model_type="qwen3") 或 create_chat_model(model_type=ModelType.QWEN3)
#   2. LLMConfig.from_model_type("qwen3") 或 LLMConfig.from_model_type(ModelType.QWEN3)
#   3. LLMConfig.from_qwen3_env() 直接创建 Qwen3 配置
# Qwen3 提供 OpenAI 兼容接口，默认使用阿里云 DashScope 的兼容模式
QWEN3_API_KEY=your_qwen_api_key_here
QWEN3_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
# 可选模型：qwen-plus, qwen-max, qwen-turbo 等
QWEN3_MODEL=qwen-plus

